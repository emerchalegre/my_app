
<h1><%= title %></h1>
<p>Welcome to <%= title %></p>

<ul>
  <% users.forEach(function(user) { %>
    <li>
      <%= user.username %>
    </li>
  <% }) %>
</ul>

<form action="/users" method="post">
  <fieldset>
    <legend>Create a new user</legend>
    <label>
      Username:
      <input type="string" name="username" />
      <input type="submit" value="Create user" />
    </label>
  </fieldset>
</form>